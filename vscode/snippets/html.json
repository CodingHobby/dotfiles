{
  "Bootstrap grid": {
    "prefix": "boot",
    "body": [
      "<div class='container'>",
      "\t<div class='row'>",
      "\t\t<div class='col-lg-12'>",
      "\t\t\t$0",
      "\t</div>",
      "</div>"
    ],
    "description": "Creates a bootstrap grid template"
  },
  "Jumbotron": {
    "prefix": "jumbo",
    "body": [
      "<div class='jumbotron'>",
      "\t<h1 class='jumbotron-heading text-center'>",
      "\t\t${1:Title}",
      "\t</h1>",
      "\t<p class='lead'>",
      "\t\t${0:Lorem Ipsum}",
      "\t</p>",
      "</div>"
    ],
    "description": "Creates a bootstrap Jumbotron"
  }
}